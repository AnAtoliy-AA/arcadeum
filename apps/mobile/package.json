{
  "name": "mobile",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "dev": "node ./scripts/dev.js",
    "start": "expo start",
    "start:localhost": "expo start --host localhost",
    "android:reverse": "node ./scripts/android-reverse.js",
    "android:open": "node ./scripts/open-android-dev-client.js",
    "android:dev": "pnpm run android:reverse && pnpm run start:localhost && pnpm run android:open",
    "android:clean": "cd android && ./gradlew clean",
    "android:assemble": "cd android && ./gradlew assembleDebug",
    "android:install": "cd android && ./gradlew installDebug",
    "android:check-redirect": "node ./scripts/check-android-redirect.js",
    "ios:check-redirect": "node ./scripts/check-ios-redirect.js",
    "ios:open": "node ./scripts/open-ios-dev-client.js",
    "reset-project": "node ./scripts/reset-project.js",
    "export:web": "expo export --platform web --output-dir dist/web-build",
    "start:web": "npx serve dist/web-build --single --listen ${PORT:-3000}",
    "build:artifacts": "pnpm run export:web && pnpm exec eas build --profile preview --platform android --non-interactive && pnpm exec eas build --profile preview --platform ios --non-interactive",
    "android": "pnpm run android:reverse && pnpm run android:install && pnpm run android:open",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "format": "prettier --write \"{api,app,components,constants,hooks,lib,pages,scripts,stores}/**/*.{js,jsx,ts,tsx,json}\"",
    "build": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@noble/ciphers": "^2.1.1",
    "@react-native-community/slider": "^5.1.1",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "axios": "^1.13.0",
    "expo": "54.0.22",
    "expo-auth-session": "^7.0.8",
    "expo-blur": "~15.0.7",
    "expo-clipboard": "~8.0.7",
    "expo-constants": "~18.0.10",
    "expo-crypto": "^15.0.7",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.14",
    "expo-secure-store": "^15.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "~15.0.9",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-app-auth": "^8.0.3",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-gifted-chat": "^2.8.1",
    "react-native-keyboard-controller": "1.18.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-uuid": "^2.0.3",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/cli": "^54.0.7",
    "@jest/globals": "^30.2.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.10",
    "dotenv": "^17.2.2",
    "eas-cli": "^16.26.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.13",
    "prettier": "^3.4.2",
    "react-native-svg-transformer": "^1.5.1",
    "react-test-renderer": "19.1.0",
    "serve": "^14.2.1",
    "typescript": "~5.9.2"
  },
  "private": true
}
