# Backend API base URL
NEXT_PUBLIC_API_BASE_URL=http://localhost:4000

# WebSocket base URL (optional, defaults to API base URL)
NEXT_PUBLIC_WS_BASE_URL=

# Socket Encryption (AES-256-GCM)
# Set to 'true' to enable socket message encryption
# Key exchange happens automatically via ECDH on socket connect
NEXT_PUBLIC_SOCKET_ENCRYPTION_ENABLED=false

# Social Media
NEXT_PUBLIC_SOCIAL_INSTAGRAM=
NEXT_PUBLIC_SOCIAL_FACEBOOK=
NEXT_PUBLIC_SOCIAL_YOUTUBE=
NEXT_PUBLIC_SOCIAL_LINKEDIN=
