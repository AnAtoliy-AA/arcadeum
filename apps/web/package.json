{
  "name": "web",
  "version": "1.1.3",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"node scripts/patch-next-config.js && storybook dev -p 6006 --no-open\" --names \"next,storybook\" --prefix-colors \"blue,magenta\"",
    "dev:next": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "type-check": "tsc --noEmit",
    "storybook": "node scripts/patch-next-config.js && storybook dev -p 6006",
    "build-storybook": "node scripts/patch-next-config.js && storybook build",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@ducanh2912/next-pwa": "^10.2.9",
    "@tanstack/react-query": "^5.90.16",
    "next": "16.1.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "socket.io-client": "^4.8.1",
    "styled-components": "^6.1.12",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^5.0.0",
    "@playwright/test": "^1.58.0",
    "@storybook/addon-a11y": "^10.2.1",
    "@storybook/addon-docs": "^10.2.1",
    "@storybook/addon-onboarding": "^10.2.1",
    "@storybook/nextjs-vite": "10.2.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^20",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/browser-playwright": "^4.0.16",
    "@vitest/coverage-v8": "^4.0.16",
    "babel-loader": "^10.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "16.1.0",
    "eslint-plugin-storybook": "^10.2.1",
    "jsdom": "^27.4.0",
    "playwright": "^1.58.0",
    "storybook": "^10.2.1",
    "typescript": "^5",
    "vite": "^7.3.0",
    "vite-tsconfig-paths": "^6.0.5",
    "vitest": "^4.0.16"
  }
}
